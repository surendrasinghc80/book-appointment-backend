    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.14.2",
    "nodemon": "^3.1.10",
    "sequelize": "^6.37.7",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.3"
  }
}
